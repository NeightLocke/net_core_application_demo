{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.RollingFile",
      "Serilog.Sinks.Async",
      "Serilog.Sinks.Map"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    }
  },
  "CustomSerilogVariables": {
    "PathLogBase": "C://Logs//SocialGames.TechnicalTest.ApiService//SocialGames-Log",
    "NormalLogSufix": "Normal",
    "ErrorsLogSufix": "Errors",
    "DefaultExtensionFile": ".txt"
  },
  "WebApiServiceProperties": {
    "ValidId": "eltesorodejava"
  },
  "GamesServiceProxyClient": {
    "BaseAddressClientConfiguration": "https://localhost:44389"
  },
  "AllowedHosts": "*"
}