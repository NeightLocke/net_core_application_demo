{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.RollingFile",
      "Serilog.Sinks.Async",
      "Serilog.Sinks.Map"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    }
  },
  "CustomSerilogVariables": {
    "PathLogBase": "C://Logs//SocialGames.GameService.ApiService//GameService-Log",
    "NormalLogSufix": "Normal",
    "DefaultExtensionFile": ".txt"
  },
  "AllowedHosts": "*"
}